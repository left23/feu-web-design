  {% block body %}
  <body class="{{ home.class }}">
  <div class="background-image background-image-{{ home.class }}"></div>
  {% endblock %}


  {% block header %}

    <header class="site-header navbar-fixed-top" id="top">
      <div class="container">

        {% block nav %}

          <nav>
            <ul class="navbar">
                {% for item in home.navigation %}

                  {% if loop.first %}
                    <li class="nav-item navbar-brand">
                      <a class="navbar-logo" href="/_index.html" title="Home" rel="home">
                        <img src="/assets/images/logo-2018.svg" alt="frontend united logo" />
                      </a>
                      <a class="navbar-logo mobile" href="/_index.html" title="Home" rel="home">
                        <img src="/assets/images/logo-white.svg" alt="frontend united logo" />
                      </a>
                    </li>
                  {% endif %}

                  <li class="nav-item">
                    <a href="/_{{ item.id }}.html" class="nav-link" title="{{ item.title }}">{{ item.title }}</a>
                  </li>
                {% endfor %}
            </ul>
          </nav>

        {% endblock %}

      </div>
    </header>

  {% endblock %}

  {% block hero %}

      <div class="hero {{ home.class }}">

        <div class="hero__bgimage-overlay"></div>

        <div class="hero__shape">

          <div class="container">
            <h1>{{ home.title }}</h1>

            <h2>{{ home.date }}<br>{{ home.location }}</h2>

          </div>

        </div>

      </div>

  {% endblock %}

